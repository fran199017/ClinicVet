<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Menu">

    <TableLayout
        android:id="@+id/tableLayout2"
        android:layout_width="360dp"
        android:layout_height="284dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.949">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="100dp">

            <ImageButton
                android:id="@+id/bCita"
                android:layout_width="121dp"
                android:layout_height="87dp"
                android:background="?attr/actionBarItemBackground"
                android:src="@drawable/pedircita" />

            <ImageButton
                android:id="@+id/bAnimales"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:background="?attr/actionBarItemBackground"
                android:src="@drawable/misanimales" />

            <ImageButton
                android:id="@+id/bAdopcion"
                android:layout_width="106dp"
                android:layout_height="match_parent"
                android:background="?attr/actionBarItemBackground"
                android:src="@drawable/adopcion" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="7dp">

            <TextView
                android:id="@+id/labelCita"
                android:layout_width="110dp"
                android:layout_height="21dp"
                android:text="Pedir Cita"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/labelAnimales"
                android:layout_width="126dp"
                android:layout_height="match_parent"
                android:text="Mis Animales"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/labelAdopcion"
                android:layout_width="111dp"
                android:layout_height="21dp"
                android:text="Adopcion"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold" />
        </TableRow>

        <TableRow
            android:layout_width="288dp"
            android:layout_height="71dp"
            android:layout_marginTop="30dp">

            <ImageButton
                android:id="@+id/bCuriosidades"
                android:layout_width="107dp"
                android:layout_height="match_parent"
                android:background="?attr/actionBarItemBackground"
                android:src="@drawable/bombilla" />

            <ImageButton
                android:id="@+id/bTienda"
                android:layout_width="77dp"
                android:layout_height="match_parent"
                android:background="?attr/actionBarItemBackground"
                android:scaleType="centerInside"
                android:src="@drawable/shop" />

            <ImageButton
                android:id="@+id/bCuidados"
                android:layout_width="106dp"
                android:layout_height="89dp"
                android:background="?attr/actionBarItemBackground"
                android:scaleType="centerInside"
                android:src="@drawable/comb" />

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="7dp">

            <TextView
                android:id="@+id/labelCuriosidades"
                android:layout_width="105dp"
                android:layout_height="match_parent"
                android:text="Curiosidades"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/labelTienda"
                android:layout_width="126dp"
                android:layout_height="match_parent"
                android:text="Tienda"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/labelTienda2"
                android:layout_width="106dp"
                android:layout_height="match_parent"
                android:text="Cuidados e higiene"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:textStyle="bold" />

        </TableRow>
    </TableLayout>

    <TextView
        android:id="@+id/labelBienvenido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bienvenido/a"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.050000012" />

    <ImageView
        android:id="@+id/imagenUsuario"
        android:layout_width="200dp"
        android:layout_height="203dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.188"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>